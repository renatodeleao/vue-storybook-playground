    "eslint-plugin-vue": "^6.1.2",
    "lint-staged": "^9.5.0",
    "prettier": "^1.19.1",
    "sass": "^1.25.0",
    "sass-loader": "^8.0.2",
    "vue-docgen-api": "^4.9.0",
    "vue-docgen-loader": "^1.4.0",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.11"
